<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

	<ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true" 
        android:scaleType = "fitXY"/>
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true" 
        android:scaleType = "fitXY"/>

    <ImageButton
        android:id="@+id/ibtn1"
        android:layout_width="140dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="300dp"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:src="@drawable/aa_03_03_btn_27_inactive_298x213" />
    
    <ImageButton
            android:id="@+id/ibtn2"
            android:layout_width="140dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/ibtn1"
            android:layout_below="@+id/ibtn1"
            android:layout_marginTop="15dp"
            android:background="#00000000"
            android:scaleType="fitXY"
            android:src="@drawable/aa_03_03_btn_54inactive_298x213" />

        <ImageButton
            android:id="@+id/ibtn3"
            android:layout_width="140dp"
            android:layout_height="100dp"
            android:layout_below="@+id/ibtn2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:src="@drawable/aa_03_03_btn_81_inactive_298x213" 
            android:background="#00000000"
            android:scaleType="fitXY"/>

        <ImageButton
            android:id="@+id/ibtn4"
            android:layout_width="140dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/ibtn3"
            android:layout_below="@+id/ibtn3"
            android:layout_marginTop="15dp"
            android:background="#00000000"
            android:scaleType="fitXY"
            android:src="@drawable/aa_03_03_btn_108_inactive_298x213" />

</RelativeLayout>

